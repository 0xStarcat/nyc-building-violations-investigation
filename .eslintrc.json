{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "no-console": 0,
    "quotes": ["warn", "single", { "avoidEscape": true }],
    "semi": ["warn", "never"],
    "no-console": "warn"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended"
}
